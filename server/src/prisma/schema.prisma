
generator client {
  provider = "prisma-client-js"
}

datasource db {
   provider = "postgresql"
    url      = env("DATABASE_URL")
}
model News{
  id Int @id @default(autoincrement())

  User User[]
}

model User{
  id Int @id @default(autoincrement())
  name String 
  lastname String
  email String @unique
  password String
  savednews News @relation(fields: [id],references: [id])
}
